{
    "compilerOptions": {
      "outDir": "dist/server",
      "rootDir": "server",
      "target": "ES2020",
      "module": "CommonJS",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["server/**/*"],
    "exclude": ["node_modules", "**/*.test.ts"]
  }
  